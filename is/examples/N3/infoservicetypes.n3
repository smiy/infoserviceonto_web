ist:knowledge_base
      rdf:type is:InfoServiceType ;
      rdfs:isDefinedBy ist: ;
      dc:description """A knowledge base is a special kind of database for knowledge management, providing the means for the 
computerized collection, organization, and retrieval of knowledge."""@en ;
